    private void submitRegistrationFormActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_submitRegistrationFormActionPerformed
        try {
            // TODO add your handling code here:
            
            //Personal Information
            Statement stmt = null;
            Random customerIdGenerator = new Random();
            int customer_id = customerIdGenerator.nextInt(50);
            String customerName = this.customerName.getText();
            int customerMobileNumber = Integer.parseInt(this.customerMobileNumber.getText());
            int customerAlternativeMobileNumber = Integer.parseInt(this.customerAlternativeMobileNumber.getText());
            String recommendedBy = this.recommendedBy.getText();
            String surety = this.surety.getText();
            String originalDocument = this.originalDocument.getText();
            int vechicleNumber = Integer.parseInt(this.vechicleNumber.getText());
            String customerAddress = this.customerAddress.getText();
            
            //Insert into customer
            
            Connection conn = Sqlliteconnection.gerSqlLiteConnection();
            try {
                stmt = conn.createStatement();
                stmt.executeUpdate("insert into customer_personal_information(customer_id,customer_Name,customer_Mobile_Number,customer_Alternative_Mobile_Number,recommended_By,surety,original_Document,vechicle_Number,customerAddress) values('"+customer_id+"','"+customerName+"','"+customerMobileNumber+"','"+customerAlternativeMobileNumber+"','"+recommendedBy+"','"+surety+"','"+originalDocument+"','"+vechicleNumber+"','"+customerAddress+"')");
                JOptionPane.showMessageDialog(null, "Personal Information Saved Successfully");
            } catch (SQLException ex) {
                Logger.getLogger(registrationForm.class.getName()).log(Level.SEVERE, null, ex);
            }
            
